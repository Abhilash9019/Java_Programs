
// remove duplicates
import java.util.Scanner;

public class removeduplicate {
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        String a=sc.nextLine();
        String temp="";

        for (int i=0; i<=a.length()-1; i++){
            char ch=a.charAt(i);
            if(!temp.contains(ch+"")){
                temp+=ch;

            }

        }
        System.out.println(temp);
    }
}
